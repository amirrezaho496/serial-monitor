<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--text box Colors-->
    <SolidColorBrush x:Key="DefaultTextColor" Color="White"/>
    <SolidColorBrush x:Key="SuccessColor" Color="LightGreen"/>
    <SolidColorBrush x:Key="FailColor" Color="OrangeRed"/>
    <SolidColorBrush x:Key="InfoColor" Color="LightBlue"/>
    <SolidColorBrush x:Key="ReceiveColor" Color="White"/>
    <SolidColorBrush x:Key="SentColor" Color="#FFF2FF92"/>
    <SolidColorBrush x:Key="TimeColor" Color="#FFFFBFBF"/>
    <SolidColorBrush x:Key="TextBoxSuccessColor" Color="#FF40FF40"/>
    <SolidColorBrush x:Key="TextBoxFailColor" Color="#FFFF4141"/>

    <!--lisbox colors-->
    <SolidColorBrush x:Key="ListboxSelectedBack" Color="#FF3B5467"/>
    <SolidColorBrush x:Key="ListboxSelectedFill" Color="#FF84BBFD"/>
    <SolidColorBrush x:Key="ListboxMouseoverBack" Color="#FF646464"/>
    <SolidColorBrush x:Key="ListboxMouseoverFill" Color="#FFC6C6C6"/>

    <!--Icons-->
    <BitmapImage  x:Key="CopyIcon"   UriSource="/Themes/Icons/copy_w.png"/>
    <BitmapImage  x:Key="AddIcon"    UriSource="/Themes/Icons/add_w.png"/>
    <BitmapImage  x:Key="RemoveIcon" UriSource="/Themes/Icons/bin_w.png"/>
    
</ResourceDictionary>
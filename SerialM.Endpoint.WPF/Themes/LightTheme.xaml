<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--text box Colors-->
    <SolidColorBrush x:Key="DefaultTextColor" Color="Black"/>
    <SolidColorBrush x:Key="SuccessColor" Color="#FF005300"/>
    <SolidColorBrush x:Key="FailColor" Color="#FF5E0000"/>
    <SolidColorBrush x:Key="InfoColor" Color="#FF00278D"/>
    <SolidColorBrush x:Key="ReceiveColor" Color="Black"/>
    <SolidColorBrush x:Key="SentColor" Color="#FF5D6F00"/>
    <SolidColorBrush x:Key="TimeColor" Color="#FF5C4242"/>

    <!--lisbox colors-->
    <SolidColorBrush x:Key="ListboxSelectedBack" Color="#d0ebff"/>
    <SolidColorBrush x:Key="ListboxSelectedFill" Color="#2a84f1"/>
    <SolidColorBrush x:Key="ListboxMouseoverBack" Color="#FFC6C6C6"/>
    <SolidColorBrush x:Key="ListboxMouseoverFill" Color="#FF646464"/>

</ResourceDictionary>